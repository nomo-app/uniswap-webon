    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter Web on Custom Port",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                // "-d",
                // "web-server",
                "--web-port",
                "3000"
            ]
        },
        {
            "name": "swap_theme_webon (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "swap_theme_webon (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}